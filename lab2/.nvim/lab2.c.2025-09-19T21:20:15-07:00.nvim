#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <sys/types.h>
#include <unistd.h>

int main() {
  printf("Enter programs to run.\n");
  char *input = NULL;
  size_t len = 0;
  getline(&input, &len, stdin);
  char *saveptr = NULL;
  char *token = NULL;

  token = strtok_r(input, '\n', &saveptr);
  printf("%s", token);

  execl(token, token, (char *)NULL);
  printf("Executed command\n");
  free(input);
}
