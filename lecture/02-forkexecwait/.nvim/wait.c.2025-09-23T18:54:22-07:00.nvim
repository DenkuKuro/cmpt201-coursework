#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>
#include <sys/wait.h>
int main() {
  pid_t pid = fork();

  int wstatus = 0;
  if (pid == 0 ) {
    execl("/bin/ls", "/bin/ls", "-a", "-l", (char*) NULL);
  } else {
    waitpid(pid, &wstatus, 0);
  }

  if (WIFEXITED(wstatus)) {
    printf("%d\n", WEXITSTATUS(wstatus));
  }
}
